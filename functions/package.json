{
  "name": "functions",
  "version": "1.0.0",
  "description": "Cloud Functions for AI Fitness App",
  "scripts": {
    "lint": "eslint --ext .ts src/",
    "lint:fix": "eslint --ext .ts src/ --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rimraf lib",
    "prebuild": "npm run clean",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "npm run lint && npm run build && firebase deploy --only functions",
    "deploy:dev": "npm run build && firebase deploy --only functions --project dev",
    "deploy:staging": "npm run build && firebase deploy --only functions --project staging",
    "deploy:prod": "npm run lint && npm run build && firebase deploy --only functions --project production",
    "logs": "firebase functions:log",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --config jest.integration.config.js",
    "test:integration:watch": "jest --config jest.integration.config.js --watch",
    "test:all": "npm run test && npm run test:integration"
  },
  "engines": {
    "node": "24"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@google-cloud/bigquery": "^7.9.0",
    "@google-cloud/pubsub": "^5.2.0",
    "@google-cloud/tasks": "^5.5.0",
    "archiver": "^7.0.1",
    "firebase-admin": "^13.6.0",
    "firebase-functions": "^7.0.0"
  },
  "devDependencies": {
    "@types/archiver": "^7.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.1",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "eslint": "^8.57.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "firebase-functions-test": "^3.4.1",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.4.5",
    "typescript": "^5.7.3"
  },
  "private": true,
  "keywords": [
    "firebase",
    "cloud-functions",
    "fitness",
    "ai"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs"
}
